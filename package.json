{
  "name": "openapi2swagger",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "devDependencies": {
    "api-spec-converter": "^2.8.3",
    "mkdirp": "^0.5.1"
  },
  "scripts": {
    "convert": "mkdirp target/swagger && api-spec-converter --from=openapi_3 --to=swagger_2 --syntax=yaml src/docs/openapi/petstore.yaml > target/swagger/petstore.yaml"
  }
}
